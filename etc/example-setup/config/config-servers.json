[
  {
    "name":          "insecure",
    "hostnames":     ["*"],
    "interface":     "*",
    "port":          8080,
    "protocol":      "http",
    "rateLimiter":   "limiter",
    "requestLogger": "requests",
    "mounts": [
      {
        "app": "my-wacky-redirector",
        "at":  "//*/"
      }
    ]
  },
  {
    "name":          "secure",
    "hostnames":     ["*"],
    "interface":     "*",
    "port":          8443,
    "protocol":      "http2",
    "rateLimiter":   "limiter",
    "requestLogger": "requests",
    "mounts": [
      {
        "app": "my-static-fun",
        "at":  "//*/"
      },
      {
        "app": "my-static-fun",
        "at":  "//*/florp/"
      }
    ]
  },
  {
    "name":          "also-secure",
    "hostnames":     ["*"],
    "interface":     "*",
    "port":          8444,
    "protocol":      "https",
    "requestLogger": "requests",
    "mounts": [
      {
        "app": "my-static-fun",
        "at":  "//*/"
      }
    ]
  }
]
