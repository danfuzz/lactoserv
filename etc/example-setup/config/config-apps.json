[
  {
    "name":      "my-wacky-redirector",
    "mount":     "//*/",
    "type":      "redirect-server",
    "redirects": [
      {
        "fromPath": "/",
        "toUri":    "https://milk.com/boop/"
      }
    ]
  },
  {
    "name":       "my-static-fun",
    "mount":      "//*/",
    "type":       "static-server",
    "assetsPath": { "$filePath": "../assets" }
  }
]
