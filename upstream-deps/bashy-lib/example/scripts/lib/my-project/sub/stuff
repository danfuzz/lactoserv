# SPDX-License-Identifier: Apache-2.0

#
# This is an example script.
#

. "$(dirname "$(readlink -f "$0")")/_init.sh" || exit "$?"

include-lib my-lib

info-msg "Hello from $(this-cmd-name)!"

if (( $# == 0 )); then
    info-msg 'No arguments.'
else
    info-msg 'Arguments:'
    info-msg --exec printf '  %q\n' "$@"
fi
